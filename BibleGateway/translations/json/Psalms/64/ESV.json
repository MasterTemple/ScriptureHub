[[],[{"type":"header","content":"Hide Me from the Wicked"},{"type":"prose","content":"To the choirmaster. A Psalm of David."},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Hide me from the secret plots of the wicked,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"who whet their tongues like swords,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"shooting from ambush at the blameless,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They hold fast to their evil purpose;"},{"type":"poetry","content":""},{"type":"poetry","content":"thinking, “Who can see them?”"},{"type":"poetry","content":""},{"type":"poetry","content":"saying, “We have accomplished a diligent search.”"},{"type":"poetry","content":""},{"type":"prose","content":"Cross references\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n"}]]