[[],[{"type":"header","content":"Psalm 86"},{"type":"header","content":"Supplication for Help against Enemies"},{"type":"prose","content":"A Prayer of David."}],[{"type":"poetry","content":"Incline your ear, O , and answer me,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Preserve my life, for I am devoted to you;"},{"type":"poetry","content":""},{"type":"poetry","content":"You are my God; be gracious to me, O Lord,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Gladden the soul of your servant,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For you, O Lord, are good and forgiving,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Give ear, O , to my prayer;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"In the day of my trouble I call on you,"},{"type":"poetry","content":""}]]