[[],[{"type":"header","content":"Psalm 136[a]"},{"type":"poetry","content":"for his loyal love endures."}],[{"type":"poetry","content":"Give thanks to the God of gods,"},{"type":"poetry","content":"for his loyal love endures."}],[{"type":"poetry","content":"Give thanks to the Lord of lords,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who performs magnificent, amazing deeds all by himself,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who used wisdom to make the heavens,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who spread out the earth over the water,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who made the great lights,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"the sun to rule by day,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"the moon and stars to rule by night,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who struck down the firstborn of Egypt,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and led Israel out from their midst,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"with a strong hand and an outstretched arm,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who divided the Red Sea in two,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and led Israel through its midst,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and tossed Pharaoh and his army into the Red Sea,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who led his people through the wilderness,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who struck down great kings,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and killed powerful kings,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"Sihon, king of the Amorites,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"Og, king of Bashan,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and gave their land as an inheritance,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"as an inheritance to Israel his servant,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who remembered us when we were down,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"and snatched us away from our enemies,"},{"type":"poetry","content":"for his loyal love endures,"}],[{"type":"poetry","content":"to the one who gives food to all living things,"},{"type":"poetry","content":"for his loyal love endures."}],[{"type":"poetry","content":"Give thanks to the God of heaven,"},{"type":"poetry","content":"for his loyal love endures!"}]]