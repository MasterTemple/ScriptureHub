[[],[{"type":"header","content":"Unjust Judgments Rebuked."},{"type":"prose","content":"A Psalm of Asaph."},{"type":"poetry","content":"He judges in the midst of the gods."},{"type":"poetry","content":""}],[{"type":"poetry","content":"How long will you judge unjustly"},{"type":"poetry","content":"And show partiality to the wicked? "},{"type":"poetry","content":""}],[{"type":"poetry","content":"Vindicate the weak and fatherless;"},{"type":"poetry","content":"Do justice to the afflicted and destitute."},{"type":"poetry","content":""}],[{"type":"poetry","content":"Rescue the weak and needy;"},{"type":"poetry","content":"Save  from the hand of the wicked."}]]