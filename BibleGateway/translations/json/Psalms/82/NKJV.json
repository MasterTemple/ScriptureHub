[[],[{"type":"header","content":"A Plea for Justice"},{"type":"prose","content":"A Psalm of Asaph."},{"type":"poetry","content":"He judges among the gods."},{"type":"poetry","content":""}],[{"type":"poetry","content":"How long will you judge unjustly,"},{"type":"poetry","content":"And show partiality to the wicked? "},{"type":"poetry","content":""}],[{"type":"poetry","content":"Defend the poor and fatherless;"},{"type":"poetry","content":"Do justice to the afflicted and needy."},{"type":"poetry","content":""}],[{"type":"poetry","content":"Deliver the poor and needy;"},{"type":"poetry","content":"Free  from the hand of the wicked."}]]