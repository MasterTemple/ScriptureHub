[[],[{"type":"header","content":"The Lord a Fortress in Adversity"},{"type":"prose","content":"To the Chief Musician. A Psalm of David."},{"type":"poetry","content":"Let me never be ashamed;"},{"type":"poetry","content":"Deliver me in Your righteousness."}],[{"type":"poetry","content":"Bow down Your ear to me,"},{"type":"poetry","content":"Deliver me speedily;"},{"type":"poetry","content":"Be my rock of refuge,"},{"type":"poetry","content":"A fortress of defense to save me."}]]