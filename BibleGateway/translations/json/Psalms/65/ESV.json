[[],[{"type":"header","content":"O God of Our Salvation"},{"type":"prose","content":"To the choirmaster. A Psalm of David. A Song."},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"O you who hear prayer,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"When iniquities prevail against me,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Blessed is the one you choose and bring near,"},{"type":"poetry","content":""},{"type":"poetry","content":"We shall be satisfied with the goodness of your house,"},{"type":"poetry","content":""}]]