[[],[{"type":"header","content":"Prayer for Rescue and Prosperity."},{"type":"prose","content":" of David."},{"type":"poetry","content":"Who trains my hands for war,"},{"type":"poetry","content":" my fingers for battle;"},{"type":"poetry","content":""}],[{"type":"poetry","content":"My faithfulness and my fortress,"},{"type":"poetry","content":"My stronghold and my savior,"},{"type":"poetry","content":"My shield and He in whom I take refuge,"},{"type":"poetry","content":"Who subdues my people under me."},{"type":"poetry","content":""}],[{"type":"poetry","content":", what is man, that You look after him?"},{"type":"poetry","content":"Or a son of man, that You think of him?"},{"type":"poetry","content":""}],[{"type":"poetry","content":"Man is like the breath;"},{"type":"poetry","content":"His days are like a passing shadow."}]]