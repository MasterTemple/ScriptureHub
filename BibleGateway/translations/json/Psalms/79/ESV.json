[[],[{"type":"header","content":"How Long, O Lord?"},{"type":"prose","content":"A Psalm of Asaph."},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They have given the bodies of your servants"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They have poured out their blood like water"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"We have become a taunt to our neighbors,"},{"type":"poetry","content":""}]]