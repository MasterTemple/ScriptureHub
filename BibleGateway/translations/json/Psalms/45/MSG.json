[[],[{"type":"poetry","content":""},{"type":"poetry","content":"I pour it out in a poem to the king,"},{"type":"poetry","content":""},{"type":"prose","content":"* * *"}],[{"type":"poetry","content":"“You’re the handsomest of men;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"Strap your sword to your side, warrior!"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"Ride on the side of truth!"},{"type":"poetry","content":""},{"type":"prose","content":"* * *"}],[{"type":"poetry","content":"“Now listen, daughter, don’t miss a word:"},{"type":"poetry","content":""},{"type":"poetry","content":"Be —the king is wild for you."},{"type":"poetry","content":""},{"type":"poetry","content":"Wedding gifts pour in from Tyre;"},{"type":"poetry","content":""}]]