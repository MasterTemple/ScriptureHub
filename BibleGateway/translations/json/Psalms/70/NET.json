[[],[{"type":"header","content":"Psalm 70[a]"},{"type":"prose","content":"For the music director, by David; written to get God’s attention."},{"type":"poetry","content":"O , hurry and help me."}],[{"type":"poetry","content":"May those who are trying to take my life"},{"type":"poetry","content":"be embarrassed and ashamed."},{"type":"poetry","content":"May those who want to harm me"},{"type":"poetry","content":"be turned back and ashamed."}],[{"type":"poetry","content":"May those who say, “Aha! Aha!”"},{"type":"poetry","content":"be driven back and disgraced."}],[{"type":"poetry","content":"May all those who seek you be happy and rejoice in you."},{"type":"poetry","content":"May those who love to experience your deliverance say continually,"},{"type":"poetry","content":"“May God be praised!”"}],[{"type":"poetry","content":"I am oppressed and needy."},{"type":"poetry","content":"O God, hurry to me."},{"type":"poetry","content":"You are my helper and my deliverer."},{"type":"poetry","content":"O , do not delay."}]]