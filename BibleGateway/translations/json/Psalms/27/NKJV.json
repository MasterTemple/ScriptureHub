[[],[{"type":"header","content":"An Exuberant Declaration of Faith"},{"type":"prose","content":" of David."},{"type":"poetry","content":"Whom shall I fear?"},{"type":"poetry","content":"The   the strength of my life;"},{"type":"poetry","content":"Of whom shall I be afraid?"}],[{"type":"poetry","content":"When the wicked came against me"},{"type":"poetry","content":"To eat up my flesh,"},{"type":"poetry","content":"My enemies and foes,"},{"type":"poetry","content":"They stumbled and fell."}],[{"type":"poetry","content":"Though an army may encamp against me,"},{"type":"poetry","content":"My heart shall not fear;"},{"type":"poetry","content":"Though war may rise against me,"},{"type":"poetry","content":"In this I  confident."}]]