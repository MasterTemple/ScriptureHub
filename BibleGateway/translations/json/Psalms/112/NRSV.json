[[],[{"type":"header","content":"Psalm 112"},{"type":"header","content":"Blessings of the Righteous"}],[{"type":"poetry","content":"Praise the !"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Their descendants will be mighty in the land;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Wealth and riches are in their houses,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They rise in the darkness as a light for the upright;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"It is well with those who deal generously and lend,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For the righteous will never be moved;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They are not afraid of evil tidings;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Their hearts are steady, they will not be afraid;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They have distributed freely, they have given to the poor;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The wicked see it and are angry;"},{"type":"poetry","content":""},{"type":"poetry","content":""}]]