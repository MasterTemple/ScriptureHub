[[],[{"type":"header","content":"Job Continues: Still I Will Hope in God"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"What you know, I also know;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"But I would speak to the Almighty,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"As for you, you whitewash with lies;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Oh that you would keep silent,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Hear now my argument"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Will you speak falsely for God"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Will you show partiality toward him?"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Will it be well with you when he searches you out?"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"He will surely rebuke you"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Will not his majesty terrify you,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your maxims are proverbs of ashes;"},{"type":"poetry","content":""}]]