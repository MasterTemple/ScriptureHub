[[],[{"type":"header","content":"Job Continues: Where Is Wisdom?"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Iron is taken out of the earth,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Man puts an end to darkness"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"He opens shafts in a valley away from where anyone lives;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"As for the earth, out of it comes bread,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Its stones are the place of sapphires,"},{"type":"poetry","content":""}]]