[[],[{"type":"header","content":"Job Continues: A Plea to God"},{"type":"poetry","content":"I will give free utterance to my complaint;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I will say to God, Do not condemn me;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Does it seem good to you to oppress,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Have you eyes of flesh?"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Are your days as the days of man,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"that you seek out my iniquity"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"although you know that I am not guilty,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your hands fashioned and made me,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Remember that you have made me like clay;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Did you not pour me out like milk"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"You clothed me with skin and flesh,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"You have granted me life and steadfast love,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Yet these things you hid in your heart;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"If I sin, you watch me"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"If I am guilty, woe to me!"},{"type":"poetry","content":""},