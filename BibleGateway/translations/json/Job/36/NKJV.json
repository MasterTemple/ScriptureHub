[[],[{"type":"header","content":"Elihu Proclaims God’s Goodness"},{"type":"prose","content":"Elihu also proceeded and said:"},{"type":"header","content":"Elihu Proclaims God’s Majesty"}],[{"type":"poetry","content":"“Remember to magnify His work,"},{"type":"poetry","content":"Of which men have sung."},{"type":"poetry","content":""}],[{"type":"poetry","content":"Everyone has seen it;"},{"type":"poetry","content":"Man looks on  from afar."}]]