[[],[{"type":"header","content":"Elihu Rebukes Job"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"See, I open my mouth;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"My words declare the uprightness of my heart,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The spirit of God has made me,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Answer me, if you can;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"See, before God I am as you are;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"No fear of me need terrify you;"},{"type":"poetry","content":""}]]