[[],[{"type":"header","content":"Expressions of Praise"},{"type":"prose","content":"The Beloved"},{"type":"poetry","content":"O prince’s daughter!"},{"type":"poetry","content":"The curves of your thighs  like jewels,"},{"type":"poetry","content":"The work of the hands of a skillful workman."}],[{"type":"poetry","content":"Your navel  a rounded goblet;"},{"type":"poetry","content":"It lacks no blended beverage."},{"type":"poetry","content":"Your waist  a heap of wheat"},{"type":"poetry","content":"Set about with lilies."}],[{"type":"poetry","content":"Your two breasts  like two fawns,"},{"type":"poetry","content":"Twins of a gazelle."}],[{"type":"poetry","content":"Your neck  like an ivory tower,"},{"type":"poetry","content":"Your eyes  the pools in Heshbon"},{"type":"poetry","content":"By the gate of Bath Rabbim."},{"type":"poetry","content":"Your nose  like the tower of Lebanon"},{"type":"poetry","content":"Which looks toward Damascus."}],[{"type":"poetry","content":"Your head  you like  Carmel,"},{"type":"poetry","content":"And the hair of your head  like purple;"},{"type":"poetry","content":"A king  held captive by  tresses."},{"type":"prose","content":"The Shulamite"},{"type":"poetry","content":" goes  smoothly for my beloved,"},{"type":"poetry","content":"Moving gently the lips of sleepers."}],[{"type":"poetry","content":"I  my beloved’s,"},{"type":"poetry","content":"And his desire  toward me."}]]