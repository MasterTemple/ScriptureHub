[[],[{"type":"header","content":"Longing for Her Beloved"},{"type":"poetry","content":"If I found you outside, I would kiss you,"}],[{"type":"poetry","content":"I would lead you and bring you"},{"type":"poetry","content":"I would give you spiced wine to drink,"}],[{"type":"poetry","content":"His left hand is under my head,"}],[{"type":"poetry","content":"I adjure you, O daughters of Jerusalem,"},{"type":"header","content":"Final Advice"},{"type":"prose","content":"Others"}],[{"type":"poetry","content":"We have a little sister,"},{"type":"poetry","content":"What shall we do for our sister"}],[{"type":"poetry","content":"If she is a wall,"},{"type":"poetry","content":"but if she is a door,"},{"type":"prose","content":"She"}],[{"type":"poetry","content":"I was a wall,"},{"type":"poetry","content":"then I was in his eyes"},{"type":"prose","content":"He"}],[{"type":"poetry","content":"O you who dwell in the gardens,"},{"type":"prose","content":"She"}],[{"type":"poetry","content":"Make haste, my beloved,"},{"type":"poetry","content":"or a young stag"}]]