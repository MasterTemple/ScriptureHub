[[],[{"type":"prose","content":"Young Man"},{"type":"poetry","content":""},{"type":"poetry","content":"Your eyes are like doves"},{"type":"poetry","content":""},{"type":"poetry","content":"Your hair falls in waves,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your teeth are as white as sheep,"},{"type":"poetry","content":""},{"type":"poetry","content":"Your smile is flawless,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your lips are like scarlet ribbon;"},{"type":"poetry","content":""},{"type":"poetry","content":"Your cheeks are like rosy pomegranates"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your neck is as beautiful as the tower of David,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your breasts are like two fawns,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Before the dawn breezes blow"},{"type":"poetry","content":""},{"type":"poetry","content":"I will hurry to the mountain of myrrh"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"You are altogether beautiful, my darling,"},{"type":"poetry","content":""},{"type":"prose","content":"Young Woman"}],[{"type":"poetry","content":"Awake, north wind!"},{"type":"poetry","content":""},{"type":"poetry","content":"Blow on my garden"},{"type":"poetry","content":""},{"type":"poetry","content":"Come into your garden, my love;"},{"type":"poetry","content":""}]]