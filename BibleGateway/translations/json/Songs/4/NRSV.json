[[],[{"type":"header","content":"The Brideâ€™s Beauty Extolled"},{"type":"poetry","content":""},{"type":"poetry","content":"Your eyes are doves"},{"type":"poetry","content":""},{"type":"poetry","content":"Your hair is like a flock of goats,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your teeth are like a flock of shorn ewes"},{"type":"poetry","content":""},{"type":"poetry","content":"all of which bear twins,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your lips are like a crimson thread,"},{"type":"poetry","content":""},{"type":"poetry","content":"Your cheeks are like halves of a pomegranate"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your neck is like the tower of David,"},{"type":"poetry","content":""},{"type":"poetry","content":"on it hang a thousand bucklers,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your two breasts are like two fawns,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Until the day breathes"},{"type":"poetry","content":""},{"type":"poetry","content":"I will hasten to the mountain of myrrh"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"You are altogether beautiful, my love;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Come with me from Lebanon, my bride;"},{"type":"poetry","content":""},{"type":"poetry","content":"Depart from the peak of Amana,"},{"type":"poetry","content":""},{"type":"poetry","content":"from the dens of lions,"},{"type":"poetry","content":""}]]