[[],[{"type":"header","content":"Springtime Rhapsody"}],[{"type":"poetry","content":"The voice of my beloved!"},{"type":"poetry","content":""},{"type":"poetry","content":"leaping upon the mountains,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"My beloved is like a gazelle"},{"type":"poetry","content":""},{"type":"poetry","content":"Look, there he stands"},{"type":"poetry","content":""},{"type":"poetry","content":"gazing in at the windows,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"My beloved speaks and says to me:"},{"type":"poetry","content":"“Arise, my love, my fair one,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"for now the winter is past,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The flowers appear on the earth;"},{"type":"poetry","content":""},{"type":"poetry","content":"and the voice of the turtledove"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The fig tree puts forth its figs,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"Arise, my love, my fair one,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"O my dove, in the clefts of the rock,"},{"type":"poetry","content":""},{"type":"poetry","content":"let me see your face,"},{"type":"poetry","content":""},{"type":"poetry","content":"for your voice is sweet,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Catch us the foxes,"},{"type":"poetry","content":""},{"type":"poetry","content":"that ruin the vineyards—"},{"type":"poetry","content":""}]]