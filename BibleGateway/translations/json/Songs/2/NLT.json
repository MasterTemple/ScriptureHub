[[],[{"type":"prose","content":"Young Woman"},{"type":"prose","content":"Young Man"}],[{"type":"poetry","content":"Like a lily among thistles"},{"type":"prose","content":"Young Woman"}],[{"type":"poetry","content":"Like the finest apple tree in the orchard"},{"type":"poetry","content":"I sit in his delightful shade"}],[{"type":"poetry","content":"He escorts me to the banquet hall;"}],[{"type":"poetry","content":"Strengthen me with raisin cakes,"}],[{"type":"poetry","content":"His left arm is under my head,"},{"type":"prose","content":"Young Man"}],[{"type":"poetry","content":"My dove is hiding behind the rocks,"},{"type":"poetry","content":"Let me see your face;"},{"type":"poetry","content":"For your voice is pleasant,"},{"type":"prose","content":"Young Women of Jerusalem"}],[{"type":"poetry","content":"Catch all the foxes,"},{"type":"poetry","content":"before they ruin the vineyard of love,"},{"type":"prose","content":"Young Woman"}],[{"type":"poetry","content":"My lover is mine, and I am his."}],[{"type":"poetry","content":"Before the dawn breezes blow"},{"type":"poetry","content":"return to me, my love, like a gazelle"}]]