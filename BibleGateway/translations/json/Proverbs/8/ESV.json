[[],[{"type":"header","content":"The Blessings of Wisdom"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"On the heights beside the way,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"beside the gates in front of the town,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"â€œTo you, O men, I call,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"O simple ones, learn prudence;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Hear, for I will speak noble things,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"for my mouth will utter truth;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"All the words of my mouth are righteous;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They are all straight to him who understands,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Take my instruction instead of silver,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"for wisdom is better than jewels,"},{"type":"poetry","content":""}]]