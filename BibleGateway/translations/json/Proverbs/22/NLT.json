[[],[{"type":"header","content":"Sayings of the Wise"}],[{"type":"poetry","content":"Listen to the words of the wise;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For it is good to keep these sayings in your heart"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I am teaching you today—yes, you—"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I have written thirty sayings for you,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"In this way, you may know the truth"},{"type":"poetry","content":""}]]