[[],[{"type":"header","content":"A Father's Wise Instruction"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"for I give you good precepts;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"When I was a son with my father,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"he taught me and said to me,"},{"type":"poetry","content":"â€œLet your heart hold fast my words;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Get wisdom; get insight;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Do not forsake her, and she will keep you;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The beginning of wisdom is this: Get wisdom,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Prize her highly, and she will exalt you;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"She will place on your head a graceful garland;"},{"type":"poetry","content":""}]]