[[],[{"type":"header","content":"Practical Warnings"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"if you are snared in the words of your mouth,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"then do this, my son, and save yourself,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Give your eyes no sleep"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"save yourself like a gazelle from the hand of the hunter,"},{"type":"poetry","content":""},{"type":"header","content":"Warnings Against Adultery"}],[{"type":"poetry","content":"My son, keep your father's commandment,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Bind them on your heart always;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"When you walk, they will lead you;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For the commandment is a lamp and the teaching a light,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"to preserve you from the evil woman,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Do not desire her beauty in your heart,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"for the price of a prostitute is only a loaf of bread,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Can a man carry fire next to his chest"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Or can one walk on hot coals"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"So is he who goes in to his neighbor's wife;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"People do not despise a thief if he steals"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"but if he is caught, he will pay sevenfold;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"He who commits adultery lacks sense;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"He will get wounds and dishonor,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For jealousy makes a man furious,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"He will accept no compensation;"},{"type":"poetry","content":""}]]