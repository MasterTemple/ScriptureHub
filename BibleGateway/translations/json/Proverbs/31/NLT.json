[[],[{"type":"header","content":"The Sayings of King Lemuel"},{"type":"prose","content":"The sayings of King Lemuel contain this message, which his mother taught him."},{"type":"header","content":"A Wife of Noble Character"}],[{"type":"poetry","content":"Who can find a virtuous and capable wife?"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Her husband can trust her,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"She brings him good, not harm,"},{"type":"poetry","content":""}]]