[[],[{"type":"header","content":"The Imminence and Horror of the Invasion"},{"type":"poetry","content":""},{"type":"poetry","content":"Blow the trumpet in Tekoa,"},{"type":"poetry","content":""},{"type":"poetry","content":"for evil looms out of the north,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I have likened daughter Zion"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Shepherds with their flocks shall come against her."},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"“Prepare war against her;"},{"type":"poetry","content":""},{"type":"poetry","content":"“Woe to us, for the day declines,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"“Up, and let us attack by night,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For thus says the  of hosts:"},{"type":"poetry","content":"Cut down her trees;"},{"type":"poetry","content":""},{"type":"poetry","content":"This is the city that must be punished;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"As a well keeps its water fresh,"},{"type":"poetry","content":""},{"type":"poetry","content":"violence and destruction are heard within her;"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Take warning, O Jerusalem,"},{"type":"poetry","content":""},{"type":"poetry","content":"and make you a desolation,"},{"type":"poetry","content":""}]]