[[],[{"type":"header","content":"The Woe of Nineveh"},{"type":"poetry","content":"It  all full of lies  robbery."},{"type":"poetry","content":" victim never departs."},{"type":"poetry","content":""}],[{"type":"poetry","content":"The noise of a whip"},{"type":"poetry","content":"And the noise of rattling wheels,"},{"type":"poetry","content":"Of galloping horses,"},{"type":"poetry","content":"Of clattering chariots!"},{"type":"poetry","content":""}],[{"type":"poetry","content":"Horsemen charge with bright sword and glittering spear."},{"type":"poetry","content":" a multitude of slain,"},{"type":"poetry","content":"A great number of bodies,"},{"type":"poetry","content":"Countless corpses—"},{"type":"poetry","content":"They stumble over the corpses—"},{"type":"poetry","content":""}],[{"type":"poetry","content":"Because of the multitude of harlotries of the seductive harlot,"},{"type":"poetry","content":"The mistress of sorceries,"},{"type":"poetry","content":"Who sells nations through her harlotries,"},{"type":"poetry","content":"And families through her sorceries."}]]