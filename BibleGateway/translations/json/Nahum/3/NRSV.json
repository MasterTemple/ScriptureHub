[[],[{"type":"header","content":"Ruin Imminent and Inevitable"},{"type":"prose","content":"Ah! City of bloodshed,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The crack of whip and rumble of wheel,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Horsemen charging,"},{"type":"poetry","content":""},{"type":"poetry","content":"piles of dead,"},{"type":"poetry","content":""},{"type":"poetry","content":"dead bodies without end—"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Because of the countless debaucheries of the prostitute,"},{"type":"poetry","content":""},{"type":"poetry","content":"who enslaves nations through her debaucheries,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I am against you,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"and I will let nations look on your nakedness"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"I will throw filth at you"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Then all who see you will shrink from you and say,"},{"type":"poetry","content":"“Nineveh is devastated; who will bemoan her?”"},{"type":"poetry","content":""}]]