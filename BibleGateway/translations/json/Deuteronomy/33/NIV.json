[{"header":"Moses Blesses the Tribes(A)"},{"num":33,"verse":"This is the blessing that Moses the man of God pronounced on the Israelites before his death."},{"num":2,"verse":"He said:"},{"num":7,"verse":"And this he said about Judah:"},{"num":8,"verse":"About Levi he said:"},{"num":12,"verse":"About Benjamin he said:"},{"num":13,"verse":"About Joseph he said:"},{"num":18,"verse":"About Zebulun he said:"},{"num":20,"verse":"About Gad he said:"},{"num":22,"verse":"About Dan he said:"},{"num":23,"verse":"About Naphtali he said:"},{"num":24,"verse":"About Asher he said:"}]