[[],[{"type":"header","content":"Judgment on the Whole Earth"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"And it shall be, as with the people, so with the priest;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"as with the buyer, so with the seller;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The earth shall be utterly empty and utterly plundered;"},{"type":"poetry","content":""}]]