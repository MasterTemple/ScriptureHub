[[],[{"type":"header","content":"The Humiliation of Babylon"},{"type":"poetry","content":"O virgin daughter of Babylon;"},{"type":"poetry","content":"Sit on the ground without a throne,"},{"type":"poetry","content":"O daughter of the Chaldeans!"},{"type":"poetry","content":"For you shall no more be called"},{"type":"poetry","content":"Tender and delicate."},{"type":"poetry","content":""}],[{"type":"poetry","content":"Take the millstones and grind meal."},{"type":"poetry","content":"Remove your veil,"},{"type":"poetry","content":"Take off the skirt,"},{"type":"poetry","content":"Uncover the thigh,"},{"type":"poetry","content":"Pass through the rivers."},{"type":"poetry","content":""}],[{"type":"poetry","content":"Your nakedness shall be uncovered,"},{"type":"poetry","content":"Yes, your shame will be seen;"},{"type":"poetry","content":"I will take vengeance,"},{"type":"poetry","content":"And I will not arbitrate with a man.‚Äù"}]]