[[],[{"type":"header","content":"The Covenant Extended to All Who Obey"},{"type":"poetry","content":""},{"type":"poetry","content":"for soon my salvation will come,"},{"type":"poetry","content":""},{"type":"header","content":"The Corruption of Israel’s Rulers"}],[{"type":"poetry","content":"All you wild animals,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Israel’s sentinels are blind,"},{"type":"poetry","content":""},{"type":"poetry","content":"they are all silent dogs"},{"type":"poetry","content":""},{"type":"poetry","content":"dreaming, lying down,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"The dogs have a mighty appetite;"},{"type":"poetry","content":""},{"type":"poetry","content":"The shepherds also have no understanding;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"“Come,” they say, “let us get wine;"},{"type":"poetry","content":""},{"type":"poetry","content":"And tomorrow will be like today,"},{"type":"poetry","content":""}]]