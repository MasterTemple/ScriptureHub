[[],[{"type":"header","content":"A Message about Assyria"},{"type":"poetry","content":"You betray others,"},{"type":"poetry","content":"When you are done destroying,"},{"type":"poetry","content":"When you are done betraying,"}],[{"type":"poetry","content":"But , be merciful to us,"},{"type":"poetry","content":"Be our strong arm each day"}],[{"type":"poetry","content":"The enemy runs at the sound of your voice."}],[{"type":"poetry","content":"Just as caterpillars and locusts strip the fields and vines,"}]]