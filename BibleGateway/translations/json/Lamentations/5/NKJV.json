[[],[{"type":"header","content":"Prayer for Restoration"},{"type":"poetry","content":"Look, and behold our reproach!"},{"type":"poetry","content":""}],[{"type":"poetry","content":"Our inheritance has been turned over to aliens,"},{"type":"poetry","content":"And our houses to foreigners."},{"type":"poetry","content":""}],[{"type":"poetry","content":"We have become orphans and waifs,"},{"type":"poetry","content":"Our mothers  like widows."}]]