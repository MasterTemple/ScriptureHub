[[],[{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":"for they deal falsely,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"But they do not consider"},{"type":"poetry","content":""},{"type":"poetry","content":"Now their deeds surround them,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"By their wickedness they make the king glad,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"They are all adulterers;"},{"type":"poetry","content":""},{"type":"poetry","content":"whose baker does not need to stir the fire,"},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"On the day of our king the officials"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"For they are kindled like an oven, their heart burns within them;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"All of them are hot as an oven,"},{"type":"poetry","content":""},{"type":"poetry","content":"All their kings have fallen;"},{"type":"poetry","content":""},{"type":"header","content":"Futile Reliance on the Nations"}],[{"type":"poetry","content":"Ephraim has become like a dove,"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"As they go, I will cast my net over them;"},{"type":"poetry","content":""},{"type":"poetry","content":""},{"type":"poetry","content":""}],[{"type":"poetry","content":"Woe to them, for they have strayed from me!"},{"type":"poetry","content":""},{"type":"poetry","content":"I would redeem them,"},{"type":"poetry","content":""}]]