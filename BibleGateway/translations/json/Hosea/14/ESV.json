[[],[{"type":"header","content":"A Plea to Return to the Lord"}],[{"type":"poetry","content":"Take with you words"},{"type":"poetry","content":"say to him,"},{"type":"poetry","content":"accept what is good,"}],[{"type":"poetry","content":"Assyria shall not save us;"},{"type":"poetry","content":"and we will say no more, ‘Our God,’"},{"type":"poetry","content":"In you the orphan finds mercy.”"}]]