    "name": "Pulpit Commentary",
    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verses 10-16.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - The line of royal descent from David, is now rapidly carried down in these ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "verses - first",
        "children": []
