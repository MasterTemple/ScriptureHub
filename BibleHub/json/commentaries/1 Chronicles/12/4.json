  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Cambridge Bible for Schools and Colleges",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "4",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "among the thirty and over the thirty",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] Ismaiah’s name does not occur among ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "the thirty",
        "children": []
      },
      {
        "element": "span",
