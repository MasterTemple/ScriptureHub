        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "But unto",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; or, ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "in",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "in a state of Holiness",
        "children": [
          {
            "element": "span",
            "class": "",
            "text": "in a state of ",
            "children": []
          },
          {
            "element": "span",
            "class": "cmt_word",
            "text": "",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": "Holiness",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "",
        "text": "; or ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "sanctification",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; the same word as in the third verse; so that holiness is the whole sphere of cur Christian life. 1 Thessalonians 4:7",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "",
        "children": []
      }
