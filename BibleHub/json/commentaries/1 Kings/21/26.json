      {
        "element": "span",
        "class": "verse-link",
        "text": "Numbers 14:45",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", and ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
