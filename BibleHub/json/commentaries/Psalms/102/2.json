          {
            "element": "span",
            "class": "verse-link",
            "text": "Psalm 17:6",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": ", note; compare ",
            "children": []
          },
          {
            "element": "span",
            "class": "verse-link",
            "text": "Psalm 17:1",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": "; ",
            "children": []
