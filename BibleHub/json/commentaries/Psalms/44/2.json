      },
      {
        "element": "span",
        "class": "",
        "text": "2. Thus, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "make them send or shoot forth",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", to wit, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "branches",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", as it is more fully expressed, ",
