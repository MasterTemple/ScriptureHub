  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Matthew Poole's Commentary",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": " A ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "new song; ",
        "children": []
