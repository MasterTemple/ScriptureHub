[
  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(150, 151) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "Near.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "—Notice the antithesis. ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "They, the wicked, ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "are ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "near ",
        "children": []
      },
      {
        "element": "span",
