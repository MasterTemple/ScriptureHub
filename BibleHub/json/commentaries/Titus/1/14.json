    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "Not giving heed (μὴ προσέχοντες)",
        "children": []
      },
      {
        "element": "p",
        "class": "",
        "text": "Reprove sharply, that they may be sound in the faith, and may show their soundness by not giving heed, etc. See on 1 Timothy 1:4.",
        "children": [
          {
            "element": "span",
            "class": "",
            "text": "Reprove sharply, that they may be sound in the faith, and may show their soundness by not giving heed, etc. See on ",
            "children": []
          },
          {
            "element": "span",
            "class": "verse-link",
            "text": "1 Timothy 1:4",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": ".",
            "children": []
