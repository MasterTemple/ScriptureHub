        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "not be innocent",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; Hebrew, ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "lo-yinnakeh",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; ",
        "children": []
      },
      {
        "element": "span",
