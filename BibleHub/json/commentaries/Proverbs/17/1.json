        "text": " : or, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "of slain beasts, ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "as the same word, ",
        "children": []
      },
      {
