  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Pulpit Commentary",
    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 25.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Meshullam",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " and ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Obadiah",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " are new as Levitical names; but the remaining names of the passage are well known. ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Talmon and Akkub",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " are among the porters of David's time (",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "1 Chronicles 9:17",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "), and are mentioned in ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
