  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "2 Chronicles 17:11. ",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "2 Chronicles 17:11",
            "children": []
          },
          {
            "element": "span",
