      {
        "element": "span",
        "class": "ital",
        "text": "And they were crushed, nation by nation and city by city.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " The verb ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "khathath",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " occurs ",
        "children": []
      },
