          {
            "element": "span",
            "class": "",
            "text": ") is for Bani; and ",
            "children": []
          },
          {
            "element": "span",
            "class": "ital",
            "text": "the Gadite ",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": "(with the article) differs from ",
            "children": []
