      {
        "element": "span",
        "class": "verse-link",
        "text": "John 17:1",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "), and in which the angels are sharers.",
        "children": []
      },
      {
        "element": "p",
        "class": "",
        "text": "",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "",
            "children": []
          }
        ]
      }
    ]
