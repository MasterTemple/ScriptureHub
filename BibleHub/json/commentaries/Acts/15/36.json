      {
        "element": "span",
        "class": "cmt_word",
        "text": "After some days",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " is hardly equivalent to ",
        "children": []
      },
      {
        "element": "span",
        "class": "greek",
        "text": "μετά τινας",
        "children": []
      },
