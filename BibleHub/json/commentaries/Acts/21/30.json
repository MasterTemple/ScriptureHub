        "element": "span",
        "class": "verse-link",
        "text": "1 Kings 2:28",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " ff. Comp. Ewald, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "Alterth",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". p. 228 f.",
        "children": []
      },
