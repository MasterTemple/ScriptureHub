          {
            "element": "span",
            "class": "",
            "text": "About noon - ",
            "children": []
          },
          {
            "element": "span",
            "class": "verse-link",
            "text": "Acts 26:13",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": ", \"at mid-day.\" This circumstance is omitted by Luke in his account in ",
            "children": []
