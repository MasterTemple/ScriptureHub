    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 37.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - That ",
        "children": []
      },
      {
        "element": "span",
