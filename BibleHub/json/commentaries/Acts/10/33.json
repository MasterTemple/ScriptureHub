        "text": "τοῦ Θεοῦ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " for ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "σοῦ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", either from the end of the verse or from ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Acts 10:31",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".—",
