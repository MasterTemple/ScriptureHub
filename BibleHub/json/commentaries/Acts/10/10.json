            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "",
        "text": ", who is therefore said to be ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "in the Spirit.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " ",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
