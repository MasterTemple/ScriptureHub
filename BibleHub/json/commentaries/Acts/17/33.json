      {
        "element": "span",
        "class": "",
        "text": " express by the mere ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "γυνή",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "). Grotius conjectures ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
