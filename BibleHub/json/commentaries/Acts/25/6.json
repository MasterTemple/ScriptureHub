        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἐπὶ τὴν αὔριον",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Luke 10:35",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " and ",
        "children": []
      },
