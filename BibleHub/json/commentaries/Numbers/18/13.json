  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "Numbers 18:13",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "Numbers 18:13",
            "children": []
