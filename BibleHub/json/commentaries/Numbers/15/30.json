    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(30) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "That doeth ought presumptuously.—",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "Literally, ",
        "children": []
      },
      {
        "element": "span",
