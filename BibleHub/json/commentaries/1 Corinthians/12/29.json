        "element": "span",
        "class": "greekheb",
        "text": "Μὴ πάντες",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "are all?",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " [surely not]) ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "i.e.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", not very many are.—",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "δυνάμεις",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "powers",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ") viz., ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "are",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " all? For if Paul referred the ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "have all?",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " of ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "1 Corinthians 12:30",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", to it, he would have expressed it here.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
