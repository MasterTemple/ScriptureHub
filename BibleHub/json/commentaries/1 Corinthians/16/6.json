  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Pulpit Commentary",
    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 6.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Yea, and winter with you.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " This he did (",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Acts 20:3-6",
        "children": []
