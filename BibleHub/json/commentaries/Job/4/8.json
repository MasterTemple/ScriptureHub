  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "Job 4:8",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "Job 4:8",
            "children": []
          }
