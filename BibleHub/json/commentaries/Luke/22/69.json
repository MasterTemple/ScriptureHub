          {
            "element": "span",
            "class": "bld",
            "text": "Ὁ ΥἹῸς ΤΟῦ ἈΝΘΡΏΠΟΥ",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
