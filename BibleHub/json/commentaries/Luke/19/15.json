    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(15) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "It came to pass, that when he was returned.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "—See Note on ",
        "children": []
