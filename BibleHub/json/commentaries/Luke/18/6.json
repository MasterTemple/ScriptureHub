[
  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(6) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "The unjust judge.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "—Literally, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "the judge of injustice,",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " as with the unjust steward in ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Luke 16:8",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", the usual adjective giving way to the stronger, more Hebraic idiom of the characterising genitive.",
        "children": []
      },
      {
        "element": "p",
        "class": "",
        "text": "",
        "children": []
      }
    ]
