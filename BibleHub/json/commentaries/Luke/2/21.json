        "text": "of His mother",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". So ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἐν κοιλίᾳ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " is used absolutely, ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Jeremiah 1:5",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "בבטן",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
