  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Meyer's NT Commentary",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Luke 24:39",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". In the ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "first",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " half of the verse Jesus desires to remove from His disciples their ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "consternation",
        "children": []
      },
      {
        "element": "span",
