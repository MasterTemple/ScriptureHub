      {
        "element": "span",
        "class": "greekheb",
        "text": "καθημένους ε",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "π",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "., etc., sitting on the face of the earth; the language here has a Hebrew colouring.",
        "children": []
