      {
        "element": "span",
        "class": "ital",
        "text": "From Bethel to Luz — ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "Dupin translates this ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "Bethel-Luz, ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "and Dr. Waterland, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "Bethel, which is Luz; ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "for we sometimes find them mentioned as one and the same place, as ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Joshua 18:13",
        "children": []
      },
