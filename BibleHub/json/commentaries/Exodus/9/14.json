  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "Exodus 9:14-15",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "Exodus 9:14-15",
            "children": []
