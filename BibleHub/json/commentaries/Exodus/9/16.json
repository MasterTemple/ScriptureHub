        "children": [
          {
            "element": "span",
            "class": "",
            "text": "And in very deed for this ",
            "children": []
          },
          {
            "element": "i",
            "class": "",
            "text": "cause",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": " have I raised thee up, for to shew ",
            "children": []
          },
          {
            "element": "i",
            "class": "",
            "text": "in",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": " thee my power; and that my ",
            "children": []
          },
          {
            "element": "span",
            "class": "cverse3",
