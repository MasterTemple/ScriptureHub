  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Pulpit Commentary",
    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 17.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "The overlaying of their chapiters of silver",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". This is additional to what is recorded in ch. 27, and is parallel to what we find related of the tabernacle pillars in ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Exodus 36:38",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Filleted with silver",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". Rather, \"connected with silver rods.\" Compare ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Exodus 27:17",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". Exodus 38:17",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "",
        "children": []
      }
    ]
  },
  {
