  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "Exodus 23:27-28",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "Exodus 23:27-28",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "I will send my fear before thee — ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "And they that ",
        "children": []
