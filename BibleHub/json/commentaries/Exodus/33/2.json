  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Pulpit Commentary",
    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 2.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "I will send an angel before thee.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " Note the change from \"",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "my",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " angel\" (",
        "children": []
      },
