      },
      {
        "element": "span",
        "class": "",
        "text": "] See above ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "2 Kings 21:4-5",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
