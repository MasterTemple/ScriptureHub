  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Meyer's NT Commentary",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "John 7:24",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". This closing admonition is ",
        "children": []
      },
