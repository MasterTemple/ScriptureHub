        "element": "span",
        "class": "greekheb",
        "text": "Τινές",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", standing first and apart, has a special emphasis. “",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "Some",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " there were among the people, who were disposed,” etc.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἀλλʼ οὐδεὶς",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "κ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "τ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "λ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".] according to ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "John 7:30",
        "children": []
      },
      {
        "element": "span",
