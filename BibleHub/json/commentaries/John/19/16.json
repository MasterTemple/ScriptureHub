  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "John 19:16-18",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "John 19:16-18",
            "children": []
          }
