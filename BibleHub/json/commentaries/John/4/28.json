    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "John 4:28-30",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "John 4:28-30",
            "children": []
          }
