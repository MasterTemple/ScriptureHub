  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Expositor's Greek Testament",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "John 4:45",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". Neither is ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "οὖν",
        "children": []
      },
      {
        "element": "span",
