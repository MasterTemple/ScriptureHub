  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "John 10:19-21",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "John 10:19-21",
            "children": []
          }
