      {
        "element": "span",
        "class": "ital",
        "text": "to see",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " is a more intimate perception than ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "to hear",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", the ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "seeing",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " is with elegant propriety ascribed to the Son, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "the hearing",
        "children": []
      },
      {
        "element": "span",
