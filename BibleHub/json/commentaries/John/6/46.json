    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 46.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Not that any one hath seen the Father, save he who is from God, he hath seen the Father.",
