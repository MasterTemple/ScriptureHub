      },
      {
        "element": "span",
        "class": "ital",
        "text": "as",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " has its Apodosis in that clause, ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "so also He who eateth Me",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".—",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "διὰ τὸν Πατέρα",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "on account of the Father",
        "children": []
      },
      {
