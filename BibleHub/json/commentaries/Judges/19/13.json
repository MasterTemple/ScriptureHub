    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(13) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "Or in Ramah.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "—This town, now ",
        "children": []
