      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Jdg 12:7",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Jdg 12:12",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " ‘in Sebee of G.’; this suggests that the text originally ran ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "in his city, in Mizpeh of G.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", cf. ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Jdg 8:27",
        "children": []
      },
