          {
            "element": "span",
            "class": "",
            "text": "where the ",
            "children": []
          },
          {
            "element": "span",
            "class": "ital",
            "text": "and ",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": "is explanatory. “Exhausted and pursuing,” ",
            "children": []
          },
          {
            "element": "span",
            "class": "ital",
            "text": "1 ",
            "children": []
          },
          {
            "element": "span",
