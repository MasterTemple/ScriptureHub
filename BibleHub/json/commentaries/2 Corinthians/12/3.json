        "element": "span",
        "class": "ital",
        "text": "licet",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " homini loqui,” ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "i.e. which a man may not utter aloud",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". Lucian, ",
        "children": []
      },
