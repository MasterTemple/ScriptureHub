      },
      {
        "element": "span",
        "class": "ital",
        "text": "such a one",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ") ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "τὸν ἐν Χριστῷ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "him who",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " was ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "in Christ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".—",
        "children": []
      },
      {
        "element": "span",
