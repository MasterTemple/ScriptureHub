  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Cambridge Bible for Schools and Colleges",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "17. ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "make a gain of you",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] See ch. ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "2 Corinthians 2:11",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
