  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Cambridge Bible for Schools and Colleges",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "14. ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "As also",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] St Paul connects “the future for which he ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "hopes",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", with the past of which he ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "knows",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".”—Meyer.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
