  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Benson Commentary",
    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "Genesis 25:26",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "Genesis 25:26",
            "children": []
