[
  {
    "type": "EXPOSITORY (ENGLISH BIBLE)",
    "name": "Ellicott's Commentary for English Readers",
    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "(6) ",
        "children": []
      },
      {
        "element": "span",
        "class": "bld",
        "text": "He hath set me in dark places.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "—A verbal reproduction of ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Psalm 143:3",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". The “dark places” are those of hell or Hades. For ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "dead of old ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "read ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "dead eternally ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "or ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "dead for ever, ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "the adverb looking forward rather than back.",
        "children": []
      },
      {
        "element": "p",
        "class": "",
        "text": "",
        "children": [
          {
            "element": "span",
            "class": "verse-link",
            "text": "",
