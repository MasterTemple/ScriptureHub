      {
        "element": "span",
        "class": "accented",
        "text": "plus",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " one-fifth, or, if not redeemed, sold for the benefit of the sanctuary. Leviticus 27:26",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "",
        "children": []
      }
