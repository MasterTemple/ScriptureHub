      {
        "element": "span",
        "class": "verse-link",
        "text": "Leviticus 12:1-8",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", with the subject of which it is inmediately connected. Compare especially ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Leviticus 12:2",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " with ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Leviticus 15:19",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". It stands here between two chapters, with neither of which has it any close connection. ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "",
        "children": []
      }
    ]
