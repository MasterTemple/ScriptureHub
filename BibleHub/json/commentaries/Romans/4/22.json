  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Expositor's Greek Testament",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Romans 4:22",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "διὸ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ": because of this signal faith, evinced so triumphantly in spite of all there was to quell it. ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἐλογίσθη",
        "children": []
