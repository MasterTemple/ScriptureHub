      {
        "element": "span",
        "class": "",
        "text": "[700] ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "d",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " refers to the note of the commentator or editor named on the particular passage.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
