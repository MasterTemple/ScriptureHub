      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Hosea 14:4",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
