    "elements": [
      {
        "element": "span",
        "class": "",
        "text": "Against the God",
        "children": []
      },
      {
        "element": "p",
        "class": "",
        "text": "(1) of Creation Jeremiah 5:22, and",
        "children": [
          {
            "element": "span",
            "class": "",
            "text": "(1) of Creation ",
            "children": []
          },
          {
            "element": "span",
