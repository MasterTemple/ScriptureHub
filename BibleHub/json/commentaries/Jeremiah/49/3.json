        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "bld",
        "text": "king, ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " or else their supreme magistrate, with their ",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
