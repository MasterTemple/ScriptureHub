      },
      {
        "element": "p",
        "class": "",
        "text": "See on [1501]Lu 22:7-23; [1502]Lu 22:39; and see on [1503]Joh 13:10, 11; [1504]Joh 13:18, 19; [1505]Joh 13:21-30.",
        "children": []
      }
