  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Expositor's Greek Testament",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Mark 15:36",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "δραμὼν δὲ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ": if the wits were heartless mockers, then ",
        "children": []
