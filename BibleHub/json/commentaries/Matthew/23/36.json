  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Meyer's NT Commentary",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 23:36",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "Ἥξει",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] Put first for sake of emphasis: ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "shall come",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", shall inevitably come upon, etc. Comp. ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 9:15",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 27:49",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "πάντα ταῦτα",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] according to the context: ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "all this shedding of blood, i.e",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". the punishment for it.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἐπὶ τ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "γενεὰν ταύτ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".] See on ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 11:16",
        "children": []
