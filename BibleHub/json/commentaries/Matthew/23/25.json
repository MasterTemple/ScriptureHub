      },
      {
        "element": "span",
        "class": "ital",
        "text": "the cup and dish",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".—",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἁρπργῆς",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "of rapacity, extortion",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ") see ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
