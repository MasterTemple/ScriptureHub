  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Expositor's Greek Testament",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 23:25-26",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "Fifth woe",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", directed against externalism (",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Luke 11:39-41",
        "children": []
      },
      {
        "element": "span",
