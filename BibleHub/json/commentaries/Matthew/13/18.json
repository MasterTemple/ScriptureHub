  {
    "type": "EXEGETICAL (ORIGINAL LANGUAGES)",
    "name": "Meyer's NT Commentary",
    "elements": [
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 13:18",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " f. ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "Ὑμεῖς",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] emphatic, as in ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 13:16",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "οὖν",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] for it is with you precisely as has been said in ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Matthew 13:16",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ".",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          },
          {
            "element": "br",
            "class": "",
            "text": "",
            "children": []
          }
        ]
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ἀκούσατε",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "] not: ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "understand",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " (de Wette), but: ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "hear",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", attend to the parable, that is, with a view to see the ",
        "children": []
      },
      {
        "element": "span",
        "class": "ital",
        "text": "meaning",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " that it is intended to convey.",
        "children": []
      },
      {
        "element": "span",
        "class": "p",
        "text": "",
        "children": [
