    "elements": [
      {
        "element": "span",
        "class": "bld",
        "text": "20–24",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ". The Cities that repented not",
        "children": []
      },
      {
        "element": "span",
