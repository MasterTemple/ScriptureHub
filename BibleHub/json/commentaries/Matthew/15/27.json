      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "ναί",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", ",
        "children": []
      },
      {
        "element": "span",
        "class": "greekheb",
        "text": "κύριε",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "· ",
        "children": []
      },
