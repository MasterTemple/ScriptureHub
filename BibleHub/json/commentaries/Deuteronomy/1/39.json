    "elements": [
      {
        "element": "span",
        "class": "cmt_sub_title",
        "text": "Verse 39.",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": " - Only among the young of that generation should the inheritance be divided, as they had no part in the rebellion of their seniors. ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "Your little ones",
        "children": []
