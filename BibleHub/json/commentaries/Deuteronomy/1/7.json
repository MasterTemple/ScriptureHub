      {
        "element": "span",
        "class": "",
        "text": ": the ",
        "children": []
      },
      {
        "element": "span",
        "class": "accented",
        "text": "shephelah",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", or lowland, the country lying between the mountain range of Judah and the Mediterranean Sea, and stretching northwards from the parallel of Gaza to that of Carmel. ",
        "children": []
      },
      {
        "element": "span",
        "class": "cmt_word",
        "text": "In the south",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ": the ",
        "children": []
      },
      {
        "element": "span",
