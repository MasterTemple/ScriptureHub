      {
        "element": "span",
        "class": "ital",
        "text": "I am ",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "now ready to be offered” (",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "2Timothy 4:6",
        "children": []
      },
      {
        "element": "span",
        "class": "",
