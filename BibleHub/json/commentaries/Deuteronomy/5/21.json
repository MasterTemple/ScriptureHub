      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "Micah 2:2",
        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": "; ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
