        "children": []
      },
      {
        "element": "span",
        "class": "",
        "text": ", and also to the valley of the Jordan perhaps in the Wady Heshban. ",
        "children": []
      },
      {
        "element": "span",
        "class": "verse-link",
        "text": "",
        "children": []
      }
    ]
