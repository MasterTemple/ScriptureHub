          },
          {
            "element": "span",
            "class": "",
            "text": " as the spies reported, ",
            "children": []
          },
          {
            "element": "span",
            "class": "bldvs",
            "text": " Deu 1:28",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": ". See on ",
            "children": []
          },
          {
            "element": "span",
            "class": "bldvs",
            "text": " Genesis 11:4",
            "children": []
          },
          {
            "element": "span",
            "class": "",
            "text": ". ",
            "children": []
          },
          {
            "element": "span",
            "class": "p",
            "text": "",
            "children": []
          },
          {
            "element": "span",
